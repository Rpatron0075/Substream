.enter_btn {
    width: 50px;
    height: 50px;
    font-size: 20px;
    background-color: rgba(0, 0, 0, 0.5);
    color: white;
    border-width: 0;
    border-radius: 10px;
    -unity-font-style: bold;
}

.enter_btn:hover {
    width: 50px;
    height: 50px;
    font-size: 20px;
    background-color: rgba(113, 113, 113, 0.5);
    color: white;
    border-width: 0;
    border-radius: 10px;
    -unity-font-style: bold;
    scale: 1.1 1.1;
}

.enter_btn:active {
    width: 50px;
    height: 50px;
    font-size: 20px;
    background-color: rgba(36, 36, 36, 0.5);
    color: white;
    border-width: 0;
    border-radius: 10px;
    -unity-font-style: bold;
    scale: 0.9 0.9;
}
